<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <UseLightSwitchWebDeploymentProvider>True</UseLightSwitchWebDeploymentProvider>
    <AuthenticationType>None</AuthenticationType>
    <MSDeployServiceURL>http://haimobsql005</MSDeployServiceURL>
    <DeployIisAppPath>DEVAPPS/ProcurementTest</DeployIisAppPath>
    <AllowUntrustedCertificate>False</AllowUntrustedCertificate>
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <DeployAsIisApp>True</DeployAsIisApp>
    <MSDeployPublishMethod>RemoteAgent</MSDeployPublishMethod>
    <UserName>scurenton</UserName>
    <SavePWD>True</SavePWD>
    <LightSwitchPublishVersion>v1.7</LightSwitchPublishVersion>
    <CleanWPPAllFilesInSingleFolder>False</CleanWPPAllFilesInSingleFolder>
    <ExcludeGeneratedDebugSymbol>true</ExcludeGeneratedDebugSymbol>
    <AutoParameterizationWebConfigConnectionStrings>False</AutoParameterizationWebConfigConnectionStrings>
    <WPPAllFilesInSingleFolder>../Bin/Debug/app.publish/</WPPAllFilesInSingleFolder>
  </PropertyGroup>
  <Target Name="BeforeBuild">
    <MSBuild Projects="../Procurement.ls3proj" Targets="BeforeLightSwitchPublishApplication" />
  </Target>
</Project>